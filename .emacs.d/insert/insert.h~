/**
 * @file header.c
 *  
 * Description of the program objectives.
 * All necessary references.
 */

#include <stdio.h> 

/**
 * A complete description of the function.
 *
 * @param par1 description of the paramter par1.
 * @param par2 description of the paramter par2.
 * @return description of the result.
 */
int function1( int par1, char par2 ) {
  return par1*par2;
}

int main(int argc, char* argv[]) {
   /* Description the command line. eg? (How the launch the program.) */
#define USAGE "usage : %s text_file\n"
   
   int i;
   
   /* Display the command usage. */
   if ((argc > 1) && (!strcasecmp("-h",argv[1]))){
      fprintf(stderr,USAGE,argv[0]);
      return -1;
   }
   
   i=function1(i+1,0);
   
   return 0;
}
